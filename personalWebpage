<!-- Fig. 2.10: links2.html -->
<!-- Unordered list containing hyperlinks. -->
<html>
   <head>
      <meta charset = "utf-8">
      <title>Links</title>
   </head>

   <body>
      <h3 style="font-family:verdana;">My favorite site (linked below):</h3>

      <!-- create an unordered list -->
      <ul>
         <!-- the list contains four list items -->
         <li><a href = "http://www.pinterest.com">Pinterest</a></li>
       </ul>
   </body>
</html>
   <body>
